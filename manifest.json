{
  "manifest_version": 3,
  "name": "Paste Instead",
  "version": "1.0",
  "description": "Upload images from clipboard directly to the assist",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
	"storage"
    
  ],
  "background": {
    
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab"
  ]
}